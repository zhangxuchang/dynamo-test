{
    "targets"            : {
        "api-services": {
            "type" : "web",
            "hosts": "dynamo-test-dev.oasgames.com",
            "index": "web/front.php",
            "cpu": 256,
            "memory": 512
        }
    },
    "log_files": {
        "log_handler": {
            "path"                    : "/data/logs/*/",
            "age"                     : "864000",
            "aws_group"               : "zxc-dynamo-test-t:@TARGET@",
            "aws_stream"              : "dynamo-test-web",
            "datetime_format"         : "",
            "multi_line_start_pattern": "%5c%5b%5cd*%5c%5d",
            "file_fingerprint_lines"  : "1"
        }
    },
    "composer_enabled": true
}
